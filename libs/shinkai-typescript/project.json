{
  "name": "shinkai-typescript",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/shinkai-typescript/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/vite:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/shinkai-typescript",
        "main": "libs/shinkai-typescript/src/index.ts",
        "tsConfig": "libs/shinkai-typescript/tsconfig.lib.json",
        "minimal": true,
        "generateLockfile": true
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "libs/shinkai-typescript/**/*.ts",
          "libs/shinkai-typescript/package.json"
        ]
      }
    },
    "test": {
      "executor": "@nx/vite:test",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
          "config": "libs/shinkai-typescript/vite.config.ts"
      }
  }
  },
  "tags": []
}
