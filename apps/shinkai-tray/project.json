{
  "name": "shinkai-tray",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/shinkai-tray/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "defaultConfiguration": "development",
      "options": {
        "cwd": "apps/shinkai-tray",
        "commands": [
          "npx tauri build",
          "cp -rf src-tauri/target/release/bundle/ ../../dist/apps/shinkai-tray/"
        ]
      },
      "configurations": {
        "development": {},
        "production": {}
      }
    },
    "serve": {
      "executor": "nx:run-commands",
      "defaultConfiguration": "development",
      "options": {
        "cwd": "apps/shinkai-tray",
        "command": "npx tauri dev"
      },
      "configurations": {
        "development": {},
        "production": {}
      }
    },
    "lint": {
      "executor": "nx:run-commands",
      "defaultConfiguration": "development",
      "options": {
        "cwd": "apps/shinkai-tray/src-tauri",
        "command": "cargo check"
      },
      "configurations": {
        "development": {},
        "production": {}
      }
    }
  },
  "tags": []
}
